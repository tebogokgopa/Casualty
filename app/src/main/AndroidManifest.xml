        </activity>
        <activity android:name=".Activities.Login" />
        <activity android:name=".Activities.RegisterActivities.Register" />
        <activity android:name=".Activities.AdminActivities.AdminMain" />
        <activity android:name=".Activities.DoctorActivities.DoctorMain" />
        <activity android:name=".Activities.ParamedicActivities.ParamedicMain" />
        <activity android:name=".Activities.PatientActivities.PatientMain" />
        <activity android:name=".Activities.PatientActivities.AppointmentActivity" />
        <activity android:name=".Activities.PatientActivities.ViewAppointment" />
        <activity android:name=".Activities.PatientActivities.DeleteAppointment"></activity>
    </application>

</manifest>